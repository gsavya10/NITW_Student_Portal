          <div class="form-group">
            <label for="neftFiles" class="col-sm-5 control-label">Upload Photo/Scanned</label>
            <div class="col-sm-7">
              <input type="file" class="form-control" name="neftFiles" id="neftFiles" required>
            </div>
          </div>
        </div>
      </div>
      <button id="neftSbmtfBtn" type="submit" class="submit" >Submit</button>
